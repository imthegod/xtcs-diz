.crumbs
{
	display: block;
	padding-left: 0px;
}

.crumbs li
{
	display: inline;
}
.crumbs li a
{
	background: url('images/bg-crumbs.png') no-repeat 100% 0px;
	color: #2d3436;
	font: normal 12px calibri;
	text-decoration: none;
	display: block;
	float: left;
	margin-left: -13px;
	padding: 7px 17px 8px 25px;
	position: relative;
}
.crumbs li a:hover
{
	background-position: 100% -48px;
	color: #2d3436;
	cursor: pointer;
}

.crumbs li a:active
{
	background-position: 100% -96px;
	color: #2d3436;
}
textarea.fast-reply, textarea.reply, input.text-field
{
	background-color: #fff;
	color: #2d3436;
	font-family: calibri;
	font-size: 13px;
	border: 0;
	border-radius: 3px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	padding: 4px;
	resize: none;
	width: 99%;
}


::-webkit-scrollbar-track{
    background: #fff;
    border-left: 1px solid rgba(0, 0, 0, 0.04);
    
}
::-webkit-scrollbar{
    width: 10px;
    height: 10px;
}
::-webkit-scrollbar-thumb{
    background: #c0392b;
    border: 2px solid #c0392b;
}
